@model List<StellerAcunMedyaAkademiPortfolio.Models.TblSocialMedia>

<footer class="page-footer">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-sm-6">
                <p>
                    Copyright
                    <script>@*document.write(new Date().getFullYear())*@</script> &copy; @DateTime.Now.Year <a href="http://www.devcrud.com" target="_blank">Portfolio Project</a>
                </p>
            </div>
            <div class="col-sm-6">
                <div class="socials">
                    @foreach (var item in Model)
                    {
                        <a class="social-item" href="@item.Url" target="_blank"><i class="@item.Icon" alt="@item.SocialMediaName"></i></a>
                    }

                    @*<a class="social-item" href="javascript:void(0)"><i class="ti-google"></i></a>
                        <a class="social-item" href="javascript:void(0)"><i class="ti-github"></i></a>
                        <a class="social-item" href="javascript:void(0)"><i class="ti-twitter"></i></a>*@
                </div>
            </div>
        </div>
    </div>
</footer>

